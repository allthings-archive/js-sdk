{
  "name": "allthings-js-sdk",
  "version": "9.0.0",
  "description": "JavaScript SDK for communication with the Allthings API",
  "keywords": [
    "allthings",
    "app"
  ],
  "homepage": "https://www.qipp.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:qipp/allthings-js-sdk.git"
  },
  "engines": {
    "node": ">=4.4.5",
    "npm": ">=3.0.0"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "qipp",
    "email": "dev@qipp.com",
    "url": "https://www.qipp.com/"
  },
  "dependencies": {
    "cookie": "0.3.1",
    "rest": "2.0.0",
    "url-parse": "1.1.8"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "nock": "9.0.7",
    "rollup": "0.41.4",
    "rollup-plugin-babel": "2.7.1",
    "snazzy": "6.0.0",
    "standard": "8.6.0"
  },
  "scripts": {
    "prebuild": "npm run std && npm test",
    "build": "rollup -c rollup.js",
    "std": "standard --verbose | snazzy",
    "test": "ava -v --fail-fast --no-cache",
    "preversion": "git pull && npm up && npm run build",
    "version": "git add -A",
    "postversion": "git push --tags origin HEAD"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "tap": true
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  }
}
