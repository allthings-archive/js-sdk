{
  "name": "allthings-js-sdk",
  "private": true,
  "version": "1.0.0",
  "description": "The web client for Allthings.",
  "keywords": [
    "allthings",
    "app"
  ],
  "homepage": "https://www.qipp.com/",
  "author": {
    "name": "qipp",
    "url": "http://www.qipp.com/"
  },
  "license": "qipp",
  "repository": {
    "type": "git",
    "url": "git@github.com:qipp/allthings-js-sdk.git"
  },
  "engines": {
    "node": ">=4.4.5",
    "npm": ">=3.0.0"
  },
  "main": "dist/index.js",
  "dependencies": {
    "cookie": "0.2.3",
    "rest": "1.3.1",
    "when": "3.7.7"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-rollup": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "eslint": "2.11.1",
    "eslint-config-standard": "5.3.1",
    "eslint-plugin-flowtype": "2.2.7",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-standard": "1.3.2",
    "nock": "8.0.0",
    "rollup": "0.29.0",
    "rollup-plugin-babel": "2.5.1"
  },
  "scripts": {
    "prebuild": "npm run lint && npm test",
    "build": "rollup -c rollup.js",
    "lint": "eslint index.js interceptor rollup.js test",
    "test": "ava -v --fail-fast --no-cache",
    "preversion": "git pull && npm up && npm lint",
    "version": "git add -A",
    "postversion": "git push --tags origin HEAD"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "tap": true
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
