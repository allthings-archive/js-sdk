{
  "name": "allthings-js-sdk",
  "version": "0.4.4",
  "description": "JavaScript SDK for communication with the Allthings API",
  "keywords": [
    "allthings",
    "app"
  ],
  "homepage": "https://www.qipp.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:qipp/allthings-js-sdk.git"
  },
  "engines": {
    "node": ">=4.4.5",
    "npm": ">=3.0.0"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "qipp",
    "email": "dev@qipp.com",
    "url": "https://www.qipp.com/"
  },
  "dependencies": {
    "cookie": "0.2.3",
    "rest": "1.3.1",
    "when": "3.7.7"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-rollup": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "nock": "8.0.0",
    "rollup": "0.29.0",
    "rollup-plugin-babel": "2.5.1",
    "snazzy": "4.0.0",
    "standard": "7.1.2"
  },
  "scripts": {
    "prebuild": "npm run std && npm test",
    "build": "rollup -c rollup.js",
    "std": "standard --verbose | snazzy",
    "test": "ava -v --fail-fast --no-cache",
    "preversion": "git pull && npm up",
    "version": "git add -A",
    "postversion": "git push --tags origin HEAD"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "tap": true
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  }
}
